systemctl stop websocket.service
systemctl daemon-reload
systemctl enable websocket.service
systemctl start websocket.service
systemctl restart websocket.service
systemctl is-active --quiet websocket.service && echo websocket.service is running
php bin/console doc:mig:mig --no-interaction

systemctl stop jitsi-admin_messenger.service
systemctl daemon-reload
systemctl enable jitsi-admin_messenger.service
systemctl start jitsi-admin_messenger.service
systemctl restart jitsi-admin_messenger.service
systemctl is-active --quiet jitsi-admin_messenger.service && echo jitsi-admin_messenger.service is running

php bin/console doc:mig:mig --no-interaction
php bin/console cache:clear

chown -R www-data:www-data /var/www/jitsi-admin/var/

service apache2 start